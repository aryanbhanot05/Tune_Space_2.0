{
  "expo": {
    "name": "tune_space_2.0",
    "slug": "tune_space_2.0",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Emotify.png",
    "scheme": "tunespace20",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "extra": {
      "EXPO_PUBLIC_SPOTIFY_CLIENT_ID": "b84a8fccacfc40e3aa72f12e3f7f7209"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Emotify.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Emotify.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
